{
  "name": "@flashbots/api-specs",
  "description": "Flashbots Public RPC API Specifications.",
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://flashbots.github.io/api-specs/",
  "main": "dist/index.js",
  "files": [
    "dist/",
    "dist/build"
  ],
  "scripts": {
    "build": "bun scripts/build.ts",
    "generate:openrpc": "bun scripts/generate-openrpc.ts",
    "clean:openrpc": "bun scripts/cleanup-openrpc.ts",
    "build:watch": "bun run generate:openrpc && bunx tsc --watch",
    "test": "bun run build && bun test",
    "coverage": "bun scripts/coverage.ts",
    "generate:overlay:staging": "OVERLAY_NETWORKS='[{\"rpcUrl\":\"https://rpc-staging.flashbots.net\",\"label\":\"Flashbots staging\"}]' bun scripts/overlay.ts",
    "generate:overlay:sepolia": "OVERLAY_NETWORKS='[{\"rpcUrl\":\"https://rpc-sepolia.flashbots.net\",\"label\":\"Flashbots sepolia\"}]' bun scripts/overlay.ts",
    "start": "bun run build",
    "prepublishOnly": "bun run build",
    "release": "bun scripts/semantic-release.ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/flashbots/api-specs.git"
  },
  "author": "",
  "license": "Apache-2.0",
  "devDependencies": {
    "@open-rpc/meta-schema": "^1.14.2",
    "@open-rpc/schema-utils-js": "^1.16.1",
    "@open-rpc/test-coverage": "^2.2.5",
    "@semantic-release/github": "^12.0.1",
    "@semantic-release/npm": "^13.1.1",
    "@xops.net/openrpc-cli": "^1.0.2",
    "ethers": "^6.13.2",
    "js-yaml": "^4.1.0",
    "jsonpath-plus": "^10.3.0",
    "json-schema-merge-allof": "^0.8.1",
    "typescript": "5.9"
  },
  "version": "0.0.0-development",
  "engines": {
    "bun": ">=1.0.0"
  },
  "packageManager": "bun@1.2.22"
}
